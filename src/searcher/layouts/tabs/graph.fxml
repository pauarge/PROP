<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="searcher.controllers.tabs.GraphController">
    <children>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false"
                onAction="#exportGraphAction" text="Exportar Graf"/>
        <Button layoutX="175.0" layoutY="102.0" mnemonicParsing="false"
                onAction="#addNodeAction" text="Afegir Node"/>
        <TextField fx:id="addNodeText" layoutX="175.0" layoutY="59.0"/>
        <ChoiceBox fx:id="choicesGraphTab" layoutX="175.0" layoutY="14.0" prefWidth="150.0">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="AUTHOR"/>
                    <String fx:value="CONF"/>
                    <String fx:value="PAPER"/>
                    <String fx:value="LABEL"/>
                    <String fx:value="TERM"/>
                </FXCollections>
            </items>
        </ChoiceBox>
    </children>
</AnchorPane>